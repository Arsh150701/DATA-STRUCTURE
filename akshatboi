#include<bits/stdc++.h>
using namespace std;
 
#define ll long long int 
#define F(i,a,b) for(long long int i = (a); i < (b) ; ++i)
#define f1(n) for(long long int i = 0 ; i < (n) ; ++i)
#define f2(n) for(long long int j = 0 ; j < (n) ; ++j)
#define vat(array) vector<ll> array

int main()
{
        ios_base::sync_with_stdio(false), cin.tie(NULL);
        ll t;
        cin>>t;
        while(t>0)
        {
            ll n,mi=INT_MAX;;
            cin>>n;
            ll arr[n],b[n];
            f1(n)
            {
                 cin>>arr[i];
                 b[i] = arr[i];
		       mi=min(mi,arr[i]);
            }
            sort(b,b+n);
            ll k=0;
            f1(n)
            {
                 if(arr[i]!=b[i]&&arr[i]%mi!=0)
                 {
                      k=1;
                      break;
                 }
            }
            if(k==1) cout<<"NO"<<'\n';
            else cout<<"YES"<<'\n';
        t--;
        }
        return 0;
}
